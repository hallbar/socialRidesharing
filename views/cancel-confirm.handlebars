<h2>You requested to cancel trip {{this.id}}</h2>
<table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Start Zip Code</th>
                <th>End Zip Code</th>
                <th>Sunday</th>
                <th>Monday</th>
                <th>Tuesday</th>
                <th>Wednesday</th>
                <th>Thursday</th>
                <th>Friday</th>
                <th>Saturday</th>
                <th>Start Time</th>
                <th>End Time</th>
            </tr>
        </thead>
        <tbody>
            {{#each result}}
            <tr>
                <td>{{this.startZip}}</td>
                <td>{{this.endZip}}</td>
                <td>{{this.sun}}</td>
                <td>{{this.mon}}</td>
                <td>{{this.tue}}</td>
                <td>{{this.wed}}</td>
                <td>{{this.thur}}</td>
                <td>{{this.fri}}</td>
                <td>{{this.sat}}</td>
                <td>{{this.startTime}}</td>
                <td>{{this.endTime}}</td>
            </tr>
            <tr>
                <td colspan="5">
                    <h2>Is this correct?</h2>
                </td>
                <td colspan="3">
                    <div align="center">
                        <a class="btn btn-default" href="/cancel?id={{this.tid}}" role="button">Confirm cancellation</a>
                    </div>
                </td>
                <td colspan="3">
                    <div align="center">
                        <a class="btn btn-default" href="/myTrips" role="button">Return to trips</a>
                    </div>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>